<div{{ attributes }}>
	{% if label %}
	{{ title_prefix }}
	{% set label = '<div class="row"><h2 class="font30 uppercase m-bottom2 text-center">' ~ label ~ '</h2>' %}
	{{ content|render|str_replace('<div class="row">', label)|raw }}
	{{ title_suffix }}
	{% else %}
	{{ content }}
	{% endif %}	
</div>