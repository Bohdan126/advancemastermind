{%
  set classes = [
    'block',
    'block-' ~ configuration.provider|clean_class,
    'block-' ~ plugin_id|clean_class,
  ]
%}
<div{{ attributes.addClass(classes) }}>
	{{ title_prefix }}
	{% if label %}
		<h5{{ title_attributes.addClass('uppercase font18 m-bottom2') }}>{{ label }}</h5>
	{% endif %}
	{{ title_suffix }}
	{% block content %}
		{{ content }}
	{% endblock %}
</div>